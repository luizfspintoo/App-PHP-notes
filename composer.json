{
    "name": "luizf/note-sync",
    "authors": [
        {
            "name": "luizfspintoo",
            "email": "luizfspintoo@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "^3.6",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "Http\\": "Http/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "XDEBUG_MODE=coverage pest --coverage"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
